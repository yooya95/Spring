<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="student1" class="DI06.Student">
		<constructor-arg value="선덕여왕"></constructor-arg>
		<constructor-arg value="30"></constructor-arg>
		<constructor-arg>
			<list>
				<value>승마</value>
				<value>정치</value>
			</list>
		</constructor-arg>
	
		<property name="height" value="162"></property>
		<property name="weight" value="55"></property>
	</bean>

	<bean id="studentInfo1" class="DI06.StudentInfo">
		<property name="student" ref="student1"></property>
	</bean>
</beans>
